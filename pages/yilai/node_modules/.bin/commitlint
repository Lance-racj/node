#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/@commitlint+cli@15.0.0/node_modules/@commitlint/cli/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/@commitlint+cli@15.0.0/node_modules/@commitlint/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/@commitlint+cli@15.0.0/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules:/Users/durantkd/Desktop/mi-workplace/node_modules:/Users/durantkd/Desktop/node_modules:/Users/durantkd/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/@commitlint+cli@15.0.0/node_modules/@commitlint/cli/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/@commitlint+cli@15.0.0/node_modules/@commitlint/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/@commitlint+cli@15.0.0/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules:/Users/durantkd/Desktop/mi-workplace/node_modules:/Users/durantkd/Desktop/node_modules:/Users/durantkd/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@commitlint+cli@15.0.0/node_modules/@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/@commitlint+cli@15.0.0/node_modules/@commitlint/cli/cli.js" "$@"
fi
