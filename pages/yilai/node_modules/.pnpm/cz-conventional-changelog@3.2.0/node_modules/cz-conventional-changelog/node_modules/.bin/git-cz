#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/commitizen@4.2.4/node_modules/commitizen/bin/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/commitizen@4.2.4/node_modules/commitizen/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/commitizen@4.2.4/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules:/Users/durantkd/Desktop/mi-workplace/node_modules:/Users/durantkd/Desktop/node_modules:/Users/durantkd/node_modules:/Users/node_modules:/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/cz-conventional-changelog@3.2.0/node_modules/cz-conventional-changelog/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/cz-conventional-changelog@3.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/commitizen@4.2.4/node_modules/commitizen/bin/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/commitizen@4.2.4/node_modules/commitizen/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/commitizen@4.2.4/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules:/Users/durantkd/Desktop/mi-workplace/node_modules:/Users/durantkd/Desktop/node_modules:/Users/durantkd/node_modules:/Users/node_modules:/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/cz-conventional-changelog@3.2.0/node_modules/cz-conventional-changelog/node_modules:/Users/durantkd/Desktop/mi-workplace/miui-13-homepage-global/node_modules/.pnpm/cz-conventional-changelog@3.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../commitizen@4.2.4/node_modules/commitizen/bin/git-cz" "$@"
else
  exec node  "$basedir/../../../../../commitizen@4.2.4/node_modules/commitizen/bin/git-cz" "$@"
fi
